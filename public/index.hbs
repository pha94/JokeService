{{#each joke}}
<div class="card">
    <div class="card-header">
        Quote
    </div>
    <div class="card-body">
        <blockquote class="blockquote mb-0">
            <p>{{setup}}</p>
            <footer class="blockquote-footer">{{punchline}}<cite title="Source Title">{{name}}</cite></footer>
        </blockquote>
    </div>
</div>
{{/each}}